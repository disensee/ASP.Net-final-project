@model DriveListViewModel

<h1>Storage Drives</h1>
@foreach (Drive d in Model.Drives)
{
    @Html.Partial("DriveSummary", d)
}

<div page-model="@Model.PagingInfo" page-action="List" page-classes-enabled="true" page-class="btn"
     page-class-normal="btn-default" page-class-selected="btn-primary" class="btn-group pull-right">
</div>
